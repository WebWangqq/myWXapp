/*成绩*/
.navTitle{position:fixed;top:0;left:0;width:100%;height:110rpx;text-align:center;line-height:110rpx;color:#888;font-size:0;border-bottom:1px solid #ddd;background-color:#fff;}
.navTitle text{display:inline-block;width:25%;font-size:32rpx;}
.navTitle text.current{position:relative;color:#ff693e;}
.navTitle text.current:after{content:'';position:absolute;bottom:-1px;left:0;width:100%;height:0;border-bottom:4rpx solid #ff693e;}

.hasList{margin-top:110rpx;overflow-y:scroll;background-color:#fff;}
.listCon {padding:0 30rpx;}
.listCon .item{width:100%;padding:30rpx 20rpx;font-size:26rpx;border-bottom:1rpx solid #dcdcdc;-webkit-box-sizing:border-box;display:-webkit-box;}
.listCon .item .logo{display:block;width:200rpx;height:200rpx;padding-right:20rpx;vertical-align:middle;}
.listCon .item .logo image{width:200rpx;height:200rpx;}
.listCon .item .infoCon{-webkit-box-flex:1;}
.listCon .item .infoCon .info{-webkit-box-sizing:border-box;display:-webkit-box;}
.listCon .item .fontW{margin-bottom:10rpx;}
.listCon .item .info .textflex{display:block;-webkit-box-flex:1;overflow:hidden;text-overflow:ellipsis;white-space:nowrap;}
.listCon .item .info .infoRight{display:block;padding-left:10rpx;text-align:right;}
.listCon .item .time{font-size:24rpx;}
.listCon .historyList{height:94rpx;text-align:center;line-height:94rpx;color:#acacac;font-size:22rpx;letter-spacing:10px; border-bottom:1rpx solid #dcdcdc;}
.noList{margin-top:384rpx;text-align:center;}
.noList image{width:227rpx;height:216rpx;margin:0 auto;}

.loading{padding:30rpx 0;text-align:center;font-size:30rpx;}
.loading image{width:40rpx;height:40rpx;margin-right:10rpx;vertical-align:middle;}