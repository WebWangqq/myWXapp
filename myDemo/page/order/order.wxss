
.headerNavItem{position: fixed;top:1px;left:0;width:100%;height:100rpx;background-color:#fcfcfc;z-index:100;}
.headerNavItem .con{height:100rpx;display:-webkit-box;-webkit-box-orient:horizontal; }
.headerNavItem .item{height:100rpx;margin-left:80rpx;}
.headerNavItem .item:last-child{width:80rpx;margin-left:0}
.headerNavItem .item text{line-height:100rpx;color:#787878;font-size:30rpx;}
.headerNavItem .item.current{position:relative;}
.headerNavItem .item.current:after{content:'';position:absolute;left:0;bottom:20rpx;width:100%;height:6rpx;background-color:#4dceee;border-radius:6rpx;}
.headerNavItem .item.current text{color:#282828}

.container{margin-top:130rpx;}
.messageCon{margin-top:30rpx;}
.messageCon .item{position:relative;border-bottom:1rpx solid #dcdcdc;}
.messageCon .item .itemText{position:relative;height:48px;padding:10px;line-height:24px;font-size:14px;background-color:#fff;z-index:2;}
.messageCon .item .itemText view{height:48px;
text-overflow: ellipsis;
display: -webkit-box;
-webkit-line-clamp: 2;
-webkit-box-orient: vertical;
overflow: hidden;
}
.messageCon .item .itemDelete{position:absolute;top:0;right:0;display:flex;align-items:center;width:14px;padding:0 20px;height:68px;background-color:#f00;z-index:1}
.messageCon .item .itemDelete text{font-size:14px;color:#fff;}

.formInfo{padding-left:.35rem;margin-top:30rpx;background-color:#fff;}
.formInfo .item{padding-right:30rpx;line-height:90rpx;border-bottom:1px solid #eaeaea;-webkit-box-sizing:border-box;display:-webkit-box;}
.formInfo .item:last-child{border-bottom:none;}
.formInfo .item label{display:block;min-width:160rpx;padding-right:10rpx;}
.formInfo .item .info{display:block;box-flex:1; -moz-box-flex:1; -webkit-box-flex:1;}
.formInfo .item .info input,.formInfo .item .info .picker{height:90rpx;}
.formInfo .item .yzm{color:#999;}
.formInfo .item .yzm.colorMain{color:#ff693e}
.formInfo .userImg{height:190rpx;padding:20rpx 30rpx 20rpx 0;}
.formInfo .userImg label{line-height:150rpx;}
.formInfo .userImg .info{text-align:right;}

.verticalCenter{height:200rpx;display:flex;align-items:center;background-color:#f00;color:#fff;}
.verticalCenter view{width:100%;}

.verticalCenter2{height:200rpx;margin-top:30rpx;display: flex;flex-flow: row wrap;align-content:space-between;background-color:#f00}
.verticalCenter2 view{width:100%}


.navigator-hover{color:#ff693e;}


.userImg image{width:150rpx;height:150rpx;border-radius:50%;}
.btnCon{margin:30rpx;text-align:center;}
.btn{display:inline-block;width:305rpx;margin:0 20rpx;color:#fff;border:0;background-color:#ccc;}
.okBtn{background-color:#ff693e}

.testBgImg{width:604rpx;height:424rpx;background-repeat:no-repeat;background-size: 604rpx 424rpx;}

